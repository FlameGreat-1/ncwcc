{
  "version": 2,
  "name": "nsw-cleaning-company-website",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains; preload"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/api/sitemap"
    },
    {
      "source": "/robots.txt",
      "destination": "/api/robots"
    }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/services/general-cleaning",
      "destination": "/services#general-cleaning",
      "permanent": true
    },
    {
      "source": "/services/deep-cleaning",
      "destination": "/services#deep-cleaning",
      "permanent": true
    },
    {
      "source": "/services/end-of-lease",
      "destination": "/services#end-of-lease-cleaning",
      "permanent": true
    },
    {
      "source": "/services/ndis",
      "destination": "/ndis",
      "permanent": true
    },
    {
      "source": "/quote-calculator",
      "destination": "/quote",
      "permanent": true
    },
    {
      "source": "/get-quote",
      "destination": "/quote",
      "permanent": true
    },
    {
      "source": "/contact-us",
      "destination": "/contact",
      "permanent": true
    },
    {
      "source": "/frequently-asked-questions",
      "destination": "/faq",
      "permanent": true
    }
  ],
  "functions": {
    "app/api/contact.js": {
      "maxDuration": 10
    },
    "app/api/quote.js": {
      "maxDuration": 10
    },
    "app/api/gallery.js": {
      "maxDuration": 5
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "VITE_APP_NAME": "NSW Cleaning Company",
      "VITE_APP_VERSION": "1.0.0",
      "VITE_API_URL": "https://nswcleaningcompany.com.au/api"
    }
  },
  "github": {
    "silent": true
  },
  "regions": [
    "syd1"
  ],
  "framework": "vite"
}
